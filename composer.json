{
    "name": "crcms/passport",
    "description": "crcms single user authentication",
	"keywords": ["passport", "crcms", "laravel", "php"],
	"homepage": "https://github.com/crcms/passport",
    "license": "MIT",
    "type": "crcms-module",
    "require": {
        "php":">=7.2.4",
        "ext-curl":"*",
        "tymon/jwt-auth": "1.0.0-rc.3"
    },
	"autoload": {
        "files": [
        ],
        "psr-4": {
            "CrCms\\Passport\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CrCms\\Passport\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CrCms\\Passport\\Providers\\PassportServiceProvider"
            ],
            "aliases": {
            },
            "dont-discover": [
                "tymon/jwt-auth"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php ../../../bin/artisan jwt:secret"
        ]
    }
}
