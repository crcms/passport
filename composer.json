{
    "name": "crcms/passport",
    "description": "crcms single user authentication",
	"keywords": ["passport", "crcms", "laravel", "php"],
	"homepage": "https://github.com/crcms/passport",
    "license": "MIT",
    "type": "library",
    "require": {
        "php":">=7.2.4",
        "ext-curl":"*",
        "lcobucci/jwt": "^3.2",
        "crcms/foundation": "^0.0.1",
        "crcms/repository": "^2.1",
        "illuminate/translation": "^5.7",
        "illuminate/container": "^5.7",
        "illuminate/support": "^5.7",
        "illuminate/events": "^5.7",
        "illuminate/config": "^5.7",
        "illuminate/hashing": "^5.7",
        "illuminate/validation": "^5.7",
        "illuminate/database": "^5.7",
        "illuminate/queue": "^5.7",
        "illuminate/console": "^5.7",
        "illuminate/cache": "^5.7",
        "illuminate/bus": "^5.7",
        "illuminate/filesystem": "^5.7",
        "illuminate/pagination": "^5.7"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.1.1",
        "fzaninotto/faker": "^1.4",
        "phpunit/phpunit": "^7.4",
        "mockery/mockery": "^1.0",
        "vlucas/phpdotenv": "^3.3 | ^2.2",
        "crcms/microservice-dispatching": "dev-master"
    },
	"autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "CrCms\\Passport\\": "src/"
        }
    },
    "autoload-dev": {
        "files": [
        ],
        "psr-4": {
            "CrCms\\Passport\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CrCms\\Passport\\Providers\\PassportServiceProvider"
            ],
            "aliases": {
            },
            "dont-discover": [
            ]
        }
    },
    "scripts": {
    }
}
